{
  "tools": {
    "python": {
      "version": "3.12.9",
      "package_name": "python3.12",
      "install_method": "package_manager"
    },
    "git": {
      "version": "latest",
      "package_name": "git",
      "install_method": "package_manager"
    },
    "build_tools": {
      "packages": ["build-essential", "cmake", "pkg-config"],
      "install_method": "package_manager"
    },
    "aria2": {
      "name": "aria2",
      "version": "latest",
      "package_name": "aria2",
      "install_method": "package_manager"
    },
    "ninja": {
      "name": "ninja",
      "version": "latest",
      "package_name": "ninja-build",
      "install_method": "package_manager"
    },
    "ccache": {
      "name": "ccache",
      "version": "latest",
      "package_name": "ccache",
      "install_method": "package_manager"
    },
    "compression_tools": {
      "packages": ["p7zip-full", "unzip"],
      "install_method": "package_manager"
    }
  },
  "repositories": {
    "comfyui": {
        "url": "https://github.com/comfyanonymous/ComfyUI.git"
    },
    "workflows": {
        "url": "https://github.com/UmeAiRT/ComfyUI-Workflows"
    }
  },
  "pip_packages": {
    "upgrade": [
      "pip",
      "wheel"
    ],
    "torch": {
      "packages": "torch==2.8.0+cu129 torchvision torchaudio",
      "index_url": "https://download.pytorch.org/whl/cu129"
    },
    "comfyui_requirements": "requirements.txt",
    "wheels": [
      {
        "name": "nunchaku-1.0.0.dev20250904+torch2.8-cp312-cp312-linux_x86_64.whl",
        "url": "https://github.com/nunchaku-tech/nunchaku/releases/download/v1.0.0dev20250904/nunchaku-1.0.0.dev20250904+torch2.8-cp312-cp312-linux_x86_64.whl"
      },
      {
        "name": "triton-3.3.0-py3-none-any",
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/refs/heads/main/whl/triton-3.3.0-py3-none-any.whl"
      }
    ],
    "standard": [
      "facexlib",
      "cython",
      "onnxruntime",
      "onnxruntime-gpu",
      "insightface"
    ],
    "pinned": [
      "numpy==1.26.4",
      "pandas",
      "transformers==4.49.0"
    ],
    "git_repos": [
      {
        "name": "xformers",
        "url": "https://github.com/facebookresearch/xformers.git",
        "commit": "main"
      },
      {
        "name": "SageAttention",
        "url": "https://github.com/thu-ml/SageAttention",
        "commit": "main"
      },
      {
        "name": "apex",
        "url": "https://github.com/NVIDIA/apex.git",
        "commit": "master",
        "install_options": "-v --disable-pip-version-check --no-cache-dir --no-build-isolation --config-settings \"--build-option=--cpp_ext\" --config-settings \"--build-option=--cuda_ext\""
      }
    ]
  },
  "files": {
    "comfy_settings": {
      "url": "https://huggingface.co/UmeAiRT/ComfyUI-Auto_installer/resolve/main/others/comfy.settings.json",
      "destination": "user/default/comfy.settings.json"
    },
    "custom_nodes_csv": {
        "url": "https://github.com/UmeAiRT/ComfyUI-Auto_installer/raw/main/scripts/custom_nodes.csv",
        "destination": "scripts/custom_nodes.csv"
    }
  }
}
